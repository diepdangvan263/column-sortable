{
  "name": "kyslik/column-sortable",
  "description": "Package for handling column sorting in Nova Framework 3",
  "keywords": [
    "sortable",
    "sorting",
    "column",
    "sort",
    "nova"
  ],
  "license": "MIT",
  "type": "package",
  "authors": [
    {
      "name": "Martin Kiesel & DiepPK",
      "email": "diepdangvan260399@gmail.com",
      "role": "Developer and maintainer"
    }
  ],
  "require": {
    "php": ">=7.0",
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "orchestra/testbench": "3.5.*"
  },
  "autoload": {
    "psr-4": {
      "Kyslik\\ColumnSortable\\": "src/ColumnSortable/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kyslik\\ColumnSortable\\ColumnSortableServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
